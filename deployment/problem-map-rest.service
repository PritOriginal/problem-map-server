[Unit]
Description=API Server
After=network.target

[Service]
User=root
Group=root
WorkingDirectory=/root/apps/problem-map-server
ExecStart=/root/apps/problem-map-server/server-rest --config=./configs/config.yaml
Restart=always
RestartSec=4
StandardOutput=inherit

[Install]
WantedBy=multi-user.target
